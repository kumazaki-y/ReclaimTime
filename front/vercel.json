{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)", 
      "dest": "/assets/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/index.html" 
    }
  ]
}

// キャッシュを無効化するためのコメント